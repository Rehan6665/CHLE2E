<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="CHL Create Patient">


	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="utils.Listeners"></listener>
	</listeners>
	<test thread-count="5"
		name="Create Patient and verify patient is created ">
		<classes>
			<class name="chl.CreateOwner" />
			<class name="chl.CreatePatient" />
			<class name="chl.CreateSensor" />
			<class name="chl.ApiClass" />
			<class name="chl.AcceptAlerts" />
			
		</classes>
	</test> <!-- Create Patient and verify patient is created -->
</suite> <!-- CHL Create Patient -->
